<!DOCTYPE HTML>
<html>

<head>
<title>珠宝展示</title>
</head>

<style>
    h1, h2, h3 { font-family: "仿宋"; }
</style>

<body>

    <% for (let user of users){ %>
        <% if (user.J_Type === '项链'){ %>
            <h2><img src='/images/项链.jpg' alt='发生错误' align="middle" width="52" height="52"/> <%= user.ID %> : <%= user.Location %> <%= user.J_Type %> <%= user.EPC_Number %></h2>  
        <% } %>
        <% if (user.J_Type === '手镯'){ %>
            <h2><img src='/images/手镯.jpg' alt='发生错误' align="middle" width="52" height="52"/> <%= user.ID %> : <%= user.Location %> <%= user.J_Type %> <%= user.EPC_Number %></h2>
        <% } %>
        <% if (user.J_Type === '戒指'){ %>
            <h2><img src='/images/戒指.jpg' alt='发生错误' align="middle" width="52" height="52"/> <%= user.ID %> : <%= user.Location %> <%= user.J_Type %> <%= user.EPC_Number %></h2>
        <% } %>
    <% } %>
</body>

</html>